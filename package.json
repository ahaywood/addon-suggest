{
  "name": "@redwoodjs/starter-standard",
  "version": "1.0.0",
  "description": "Build fast, server-driven webapps on Cloudflare with SSR, RSC, and realtime. ",
  "main": "index.js",
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite build",
    "dev": "NODE_ENV=${NODE_ENV:-development} vite dev",
    "dev:init": "rw-scripts dev-init",
    "preview": "vite preview",
    "worker:run": "rw-scripts worker-run",
    "clean": "npm run clean:vite",
    "clean:vite": "rm -rf ./node_modules/.vite",
    "release": "rw-scripts ensure-deploy-env && npm run clean && prisma generate && RWSDK_DEPLOY=1 npm run build && wrangler deploy",
    "migrate:dev": "prisma generate && wrangler d1 migrations apply DB --local",
    "migrate:prd": "wrangler d1 migrations apply DB --remote",
    "migrate:new": "rw-scripts migrate-new",
    "seed": "npm run worker:run ./src/scripts/seed.ts",
    "generate": "rw-scripts ensure-env && prisma generate && wrangler types",
    "check": "npm run generate && npm run types",
    "types": "tsc",
    "prepare": "vibe-rules install cursor",
    "merge": "node src/scripts/mergePrismaSchema.mjs",
    "routes": "npx tsx src/scripts/generateRoutes.ts",
    "addon:generate": "node src/scripts/generateAddonConfig.mjs",
    "addon:install": "node src/scripts/installAddon.mjs"
  },
  "dependencies": {
    "@prisma/adapter-d1": "~6.5.0",
    "@prisma/client": "~6.5.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-switch": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-toggle": "^1.1.8",
    "@simplewebauthn/browser": "^13.1.0",
    "@simplewebauthn/server": "^13.1.1",
    "@tailwindcss/vite": "^4.1.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.510.0",
    "resend": "^4.5.1",
    "rwsdk": "0.0.88",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.7",
    "tw-animate-css": "^1.2.9"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "prisma": "~6.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vibe-rules": "^0.2.31",
    "vite": "^6.2.6",
    "wrangler": "^4.14.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma",
      "sharp",
      "workerd"
    ]
  }
}
